        <field name="c_preferred_cust_flag" size="1" type="CHAR" primary="false">
            <gen_ProbabilityGenerator>
                <probability value="${preferred_cust_likelihood}">
                    <gen_StaticValueGenerator>
                        <value>Y</value>
                    </gen_StaticValueGenerator>
                </probability>
                <probability value="1 - ${preferred_cust_likelihood}">
                    <gen_StaticValueGenerator>
                        <value>N</value>
