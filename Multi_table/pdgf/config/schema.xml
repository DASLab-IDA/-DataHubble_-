        <field name="wr_return_quantity1" size="10" type="INTEGER" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="wr_return_quantity" />
            </gen_OtherFieldValueGenerator>
        </field>

        <field name="wr_refunded_cash" size="7" type="NUMERIC" primary="false">
            <gen_DoubleGenerator>
                <minD>0.00d</minD>
                <maxD>1000.00d</maxD>
                <decimalPlaces>2</decimalPlaces>
            </gen_DoubleGenerator>
        </field>

        <field name="wr_refunded_cash1" size="7" type="NUMERIC" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="wr_refunded_cash" />
            </gen_OtherFieldValueGenerator>
        </field>
    </table>

    <table name="store_returns">
        <size>${store_returns_size}</size>

        <field name="sr_returned_date_sk" size="19" type="BIGINT" primary="false">
            <gen_FormulaGenerator>
                <formula>(long)generator[0]</formula>
                <decimalPlaces>0</decimalPlaces>
                <gen_ReferenceGenerator choose="sameChoiceAs" from="">
                    <sameChoiceAs field="sr_ticket_number"/>
                    <reference table="store_sales" field="ss_sold_date_sk" />
                </gen_ReferenceGenerator>
            </gen_FormulaGenerator>
        </field>

        <field name="sr_returned_date_sk1" size="19" type="BIGINT" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="sr_returned_date_sk" />
            </gen_OtherFieldValueGenerator>
        </field>

        <field name="sr_item_sk" size="19" type="BIGINT" primary="true">
            <gen_LongGenerator>
                <min>0</min>
                <max>${item_size}</max>
            </gen_LongGenerator>
        </field>

        <field name="sr_item_sk1" size="19" type="BIGINT" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="sr_item_sk" />
            </gen_OtherFieldValueGenerator>
        </field>

        <field name="sr_customer_sk" size="19" type="BIGINT" primary="true">
            <gen_LongGenerator>
                <min>0</min>
                <max>${item_size}</max>
            </gen_LongGenerator>
        </field>

        <field name="sr_customer_sk1" size="19" type="BIGINT" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="sr_customer_sk" />
            </gen_OtherFieldValueGenerator>
        </field>

        <field name="sr_ticket_number" size="19" type="BIGINT" primary="false">
            <gen_LongGenerator>
                <min>0</min>
                <max>${store_sales_size}</max>
            </gen_LongGenerator>
        </field>

        <field name="sr_ticket_number1" size="19" type="BIGINT" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="sr_ticket_number" />
            </gen_OtherFieldValueGenerator>
        </field>

        <field name="sr_return_quantity" size="10" type="INTEGER" primary="false">
            <gen_LongGenerator>
                <min>0</min>
                <max>100</max>
            </gen_LongGenerator>
        </field>

        <field name="sr_return_quantity1" size="10" type="INTEGER" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="sr_return_quantity" />
            </gen_OtherFieldValueGenerator>
        </field>

        <field name="sr_return_amt" size="7" type="NUMERIC" primary="false">
            <gen_DoubleGenerator>
                <minD>0.00d</minD>
                <maxD>100000.00d</maxD>
                <decimalPlaces>2</decimalPlaces>
            </gen_DoubleGenerator>
        </field>

        <field name="sr_return_amt1" size="7" type="NUMERIC" primary="false">
            <gen_OtherFieldValueGenerator>
                <reference field="sr_return_amt" />
            </gen_OtherFieldValueGenerator>
        </field>
    </table>

    <table name="customer">
