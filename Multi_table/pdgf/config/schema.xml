        <field name="c_customer_sk" size="19" type="BIGINT" primary="true">
            <gen_IdGenerator>
                <min>${SK_ID_OFFSET}</min>
            </gen_IdGenerator>
        </field>

        <field name="c_customer_sk1" size="19" type="BIGINT" primary="false">
            <gen_OtherFieldValueGenerator>
